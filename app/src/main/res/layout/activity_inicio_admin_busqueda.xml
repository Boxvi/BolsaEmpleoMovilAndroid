<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".InicioAdminBusqueda">


    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:id="@+id/linearLayout3"
            app:layout_constraintHorizontal_bias="0.0">
        <TextView
                android:text="@string/consultas_admin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/consultar_ofertas"/>
    </LinearLayout>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:orientation="vertical"
            android:id="@+id/linearLayout2" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3">
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_margin="20dp"
                android:layout_height="150dp"
                android:orientation="horizontal"
                android:id="@+id/btn_consultar_ofertas">
            <ImageView
                    android:layout_width="140dp"
                    android:layout_height="130dp"
                    android:layout_margin="10dp"
                    android:src="@color/cardview_dark_background"
                    android:id="@+id/img_consultar_ofertas"/>
            <Button
                    android:layout_width="140dp"
                    android:layout_height="140dp"
                    android:layout_marginLeft="30dp"
                    android:layout_gravity="center"
                    android:text="@string/consultar_ofertas"
            />
        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_margin="20dp"
                android:layout_height="150dp"
                android:orientation="horizontal"
                android:id="@+id/btn_perfil_empresa">
            <ImageView
                    android:layout_width="140dp"
                    android:layout_height="130dp"
                    android:layout_margin="10dp"
                    android:src="@color/cardview_dark_background"
                    android:id="@+id/img_perfil_empresa"/>

            <Button
                    android:layout_width="140dp"
                    android:layout_height="140dp"
                    android:layout_marginLeft="30dp"
                    android:layout_gravity="center"
                    android:text="@string/perfil_empresa"

            />
        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_margin="20dp"
                android:layout_height="150dp"
                android:orientation="horizontal"
                android:id="@+id/btn_razon_social_mayus">
            <ImageView
                    android:layout_width="140dp"
                    android:src="@color/cardview_dark_background"
                    android:layout_height="130dp"
                    android:layout_margin="10dp"
                    android:id="@+id/img_razon_social"/>

            <Button
                    android:layout_width="140dp"
                    android:layout_height="140dp"
                    android:layout_marginLeft="30dp"
                    android:layout_gravity="center"
                    android:text="@string/razon_social_mayus"

            />
        </LinearLayout>


    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>


