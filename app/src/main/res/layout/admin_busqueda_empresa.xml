<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:id="@+id/btn_ver_mas">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent">
        <ImageButton
                android:gravity="right"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@android:drawable/ic_menu_revert"
                android:id="@+id/ic_baseline_arrow_back_50" android:onClick="irMain"/>
        <LinearLayout
                android:layout_width="358dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:orientation="horizontal"
                tools:ignore="MissingConstraints"
        >

            <TextView
                    android:text="CONSULTAR EMPRESAS"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/curriculum_vitae"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"/>

        </LinearLayout>

    </LinearLayout>

    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="409dp"
            android:layout_height="500dp" tools:layout_editor_absoluteY="102dp" tools:layout_editor_absoluteX="1dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>