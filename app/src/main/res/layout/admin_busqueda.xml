<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <TextView
            android:text="CONSULTAS"
            android:layout_width="203dp"
            android:layout_height="52dp" android:id="@+id/textView"
            tools:layout_editor_absoluteX="91dp"
            tools:layout_editor_absoluteY="16dp" android:textAppearance="@style/TextAppearance.AppCompat.Display1"/>
    <androidx.cardview.widget.CardView
            android:layout_width="409dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="76dp" app:cardBackgroundColor="#040303"/>
    <androidx.cardview.widget.CardView
            android:layout_width="409dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="250dp" android:id="@+id/cardView"/>
    <androidx.cardview.widget.CardView
            android:layout_width="409dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="435dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="390dp"
            android:layout_height="149dp" tools:layout_editor_absoluteY="84dp" tools:layout_editor_absoluteX="8dp">

        <Button
                android:text="Consultar Curriculum"
                android:layout_width="153dp"
                android:layout_height="121dp" android:id="@+id/btn_curriculum" tools:layout_editor_absoluteY="10dp"
                tools:layout_editor_absoluteX="228dp"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" tools:srcCompat="@tools:sample/avatars" android:id="@+id/imageView"
                tools:layout_editor_absoluteY="11dp" tools:layout_editor_absoluteX="12dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="391dp"
            android:layout_height="154dp" tools:layout_editor_absoluteY="260dp" tools:layout_editor_absoluteX="7dp">

        <ImageView
                android:layout_width="136dp"
                android:layout_height="117dp" app:srcCompat="@android:drawable/ic_dialog_dialer"
                android:id="@+id/imageView2" tools:layout_editor_absoluteY="16dp" tools:layout_editor_absoluteX="13dp"/>
        <Button
                android:text="Perfil de empresas"
                android:layout_width="157dp"
                android:layout_height="127dp" android:id="@+id/btn_perfil" tools:layout_editor_absoluteY="16dp"
                tools:layout_editor_absoluteX="228dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="389dp"
            android:layout_height="144dp" tools:layout_editor_absoluteY="449dp" tools:layout_editor_absoluteX="13dp">

        <ImageView
                android:layout_width="146dp"
                android:layout_height="127dp" app:srcCompat="@android:drawable/checkbox_on_background"
                android:id="@+id/imageView3" tools:layout_editor_absoluteY="8dp" tools:layout_editor_absoluteX="5dp"/>
        <Button
                android:text="OFERTAS LABORALES"
                android:layout_width="162dp"
                android:layout_height="127dp" android:id="@+id/btn_ofertas" tools:layout_editor_absoluteY="8dp"
                tools:layout_editor_absoluteX="221dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <ImageView
            android:layout_width="64dp"
            android:layout_height="71dp" app:srcCompat="@android:drawable/ic_menu_sort_by_size"
            android:id="@+id/img_menu" tools:layout_editor_absoluteX="346dp" tools:layout_editor_absoluteY="5dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>